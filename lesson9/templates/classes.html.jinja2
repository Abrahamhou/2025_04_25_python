{% extends "layout/base.html.jinja2" %}
{% block title %}
職能發展學院-課程規劃
{% endblock %}
{% block link%}
    {{super()}}
    <link rel="stylesheet" href="{{url_for('static', filename='css/classes.css')}}">
{% endblock %}

{% block main %}
 <div class="course-tabs-container">
        <div class="course-tabs" role="tablist">
            <button class="tab-button active" role="tab" aria-selected="true" id="general-tab"
                aria-controls="general-panel">
                一般課程
            </button>
            <button class="tab-button" role="tab" aria-selected="false" id="advanced-tab"
                aria-controls="advanced-panel">
                進階課程
            </button>
        </div>
        <div class="tab-divider">
    </div>



 <script src="{{url_for('static', filename='js/classes.js')}}"></script>

{% endblock %}